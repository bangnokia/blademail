{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-mkdir",
    {
      "identifier": "fs:scope",
      "allow": [
        "$CACHE/**/*"
      ]
    },
    "shell:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**/"
        },
        {
          "url": "http://**/"
        }
      ]
    },
    "store:default",
    "http:default"
  ]
}